# Environment Variables Template for MalluClub Discord Bot
# Copy this file to .env and fill in your actual values

# Discord Bot Configuration
DISCORD_TOKEN=your_malluclub_bot_token_here
CLIENT_ID=your_malluclub_bot_application_id_here
GUILD_ID=your_malluclub_server_id_here

# MongoDB Configuration  
MONGODB_URI=your_mongodb_atlas_connection_string_here

# Channel Configuration
WELCOME_CHANNEL_ID=your_welcome_channel_id_here
LOG_CHANNEL_ID=your_moderation_log_channel_id_here
AUTO_ROLE_ID=your_default_member_role_id_here

# Optional: Owner IDs (comma-separated for multiple owners)
# OWNER_IDS=your_user_id_here,another_owner_id_here

# Optional: Development Settings
# NODE_ENV=production